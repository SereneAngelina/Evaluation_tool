<div class = "row">

  <div class="panel panel-warning col-md-6 col-md-offset-3">

      <div class="panel-heading text-center">
        <h3>Batches</h3>
      </div>

      <div class="panel-body text-center">
        <% @batches.each do |batch| %>
            <h4>Batch<%= link_to batch.code, batch_path(batch) %></h4>
                <%= batch.starts_at %>
             <%= batch.ends_at %>

        <% end %>
      </div>

    </div>

</div>
